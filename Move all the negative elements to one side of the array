// Online C++ compiler to run C++ program online
#include <iostream>
#include<bits/stdc++.h>
int main() {
    // Write C++ code here
    
    int arr[] ={2,-12,3,8,-9,12,-6};
    int i,j=0, temp;
    
    int n =sizeof(arr)/sizeof(arr[0]);
   for(i=0;i<n;i++)
   {
      if(arr[i]<0) 
      {
          temp=arr[i];
          arr[i]=arr[j];
          arr[j]=temp;
          j++;
      }
       
   }
 for(i=0;i<n;i++)
    std::cout <<arr[i]<<"  " ;
    

    
}

// Online C++ compiler to run C++ program online
#include <iostream>

int main() {
    // Write C++ code here
    int a[]= {1,5,10,20,40,80};
    int b[] ={6,7,20,80,100};
    int c[] = {3,4,15,20,30,70,80,120};
    int i=0,j=0,k=0;
    int n = sizeof(a)/sizeof(a[0]);
    int m = sizeof(b)/sizeof(b[0]);
int l = sizeof(c)/sizeof(c[0]);
    while(i<n && j<m && k<l)
    {
       if (a[i]==b[j] && b[j] ==c[k]){
        std::cout<<a[i]<<" " ;
        i++;
        j++;
        k++; 
       } 
        if (a[i]<b[j])
        {
            
            i++;
        }
        else if  (b[j]<c[k])
        {
            
            j++;
        }
        
        else{
           
            k++;
        }
        
        
    }
  

    return 0;
}

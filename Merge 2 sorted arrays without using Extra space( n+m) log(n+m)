// Online C++ compiler to run C++ program online
#include <iostream>
#include <bits/stdc++.h>

int main() {
    // Write C++ code here
    int ar1[] ={1,3,5,7};
    int ar2[] = {2,4,6,8,9};
    int n =sizeof(ar1)/sizeof(ar1[0]);
    int m =sizeof(ar2)/sizeof(ar2[0]);
    
    int i=0;
    
    while(i<n)
    {
        if(ar1[i]>ar2[0])
        {
            std::swap(ar1[i],ar2[0]);
            std::sort(ar2,ar2+m);
        }
        i++;
    } 
    for (i=0;i<n;i++)
    std::cout<<ar1[i]<<" ";
    for(i=0;i<m;i++)
    std::cout<<ar2[i]<<" ";
    return 0;
}
